<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aquademo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <application
        android:name=".AquaDemp"
        android:allowBackup="true"
        android:icon="@drawable/iconwatebottle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ShowRemainClients" />
        <activity android:name=".OwnDetails" />
        <activity android:name=".CustomerDetails" />
        <activity android:name=".EmptyBottles" />
        <activity android:name=".CashSale" />
        <activity android:name=".CustomerList" />
        <activity android:name=".FullBottlesActivity" />
        <activity
            android:name=".ChooseActivity"
            android:icon="@drawable/iconwatebottle"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DbDashboard" />
        <activity android:name=".DeliveryBoyLogin" />
        <activity android:name=".CustomerLogin" />
    </application>

</manifest>